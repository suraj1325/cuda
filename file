https://repo.anaconda.com/archive/Anaconda3-2023.09-0-Linux-x86_64.sh 


# Remove GCC packages
sudo apt-get remove --purge gcc g++

# Remove related packages (optional)
sudo apt-get autoremove

# Remove configuration files (optional)
sudo rm -rf /etc/gcc* /etc/ld.so.conf.d/*gcc* /usr/lib/gcc /usr/include/*
